pipeline{
  agent any

  stages{
    stage("setup"){
      steps{
        git branch: 'main', url: 'https://github.com/KauanButura99/testes-e2e-ebac-shop-exercicio.git'
        sh "npm install"
      }
    }

    stage("Test"){
      steps{
        sh 'NO_COLOR=1 npm run cy:run-ci'
      }
    }

  }

}